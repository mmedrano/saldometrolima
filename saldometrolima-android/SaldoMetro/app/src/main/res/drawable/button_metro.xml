<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <!--<stroke android:width="1dip" android:color="#FFFFFF" />-->
            <!--<gradient android:angle="-90" android:startColor="#D5D900" android:endColor="#C7CB0A" />-->
            <solid android:color="#02C102"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#5e7974" />
            <solid android:color="#58857e"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <solid android:color="#016F01"/>
            <!--<gradient android:angle="-90" android:startColor="#02C102" android:endColor="#016F01"  />-->
        </shape>
    </item>
</selector>